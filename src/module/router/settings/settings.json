{
  "moduleName": "router",
  "moduleDir": "/router",
  "outputPath": "/router",
  "dependencies": {
    "koa-router": "^10.0.0"
  },
  "dependenciesMerge": {
    "default": "self",
    "koa-router": "up"
  },
  "extra": [
    {
      "type": "file",
      "source": "/readme.md",
      "target": "@src/readme.md"
    },
    {
      "type": "dir",
      "source": "/readMe",
      "target": "/readMeDir"
    }
  ]
}